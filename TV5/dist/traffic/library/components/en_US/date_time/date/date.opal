;

FULL_YMD:
	(['on' | DATE_QUALIFIER] ['"']   ([YEAR_KEYWORDS] (ONE_AND_TWO_DIGIT_YEAR_VALUE | FOUR_DIGIT_YEAR_VALUE)) ['/']  (((DATE_MONTH_TEXT|DATE_MONTH_NUMERIC)  ['/'] DATE_DAY) | (DATE_DAY ['of'] DATE_MONTH_TEXT)) ['"'])
	{attributes:'private, atomic'}

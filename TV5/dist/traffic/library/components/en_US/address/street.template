extends library object.template;

/** 
 * Handle joining street address fields.
 */
fields:
    [['#'] ~ fieldStreetNumber] ~ (fieldHighway | fieldExpressway | fieldRoad | fieldStreet | fieldBoulvard | fieldDrive | fieldParkway| fieldAvenue | fieldCircle | fieldSquare) ~ [['#'] ~ fieldAptNumber]
    {attributes: 'private, atomic'}
;




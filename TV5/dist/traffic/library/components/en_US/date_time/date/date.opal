;

FULL_YDM:
	(['on' | DATE_QUALIFIER] ['"']   ([YEAR_KEYWORDS] (ONE_AND_TWO_DIGIT_YEAR_VALUE | FOUR_DIGIT_YEAR_VALUE)) ['/']  (DATE_DAY  ['/'] (DATE_MONTH_TEXT|DATE_MONTH_NUMERIC)) ['"'])
	{attributes:'private, atomic'}
	{action: DATE_DAY, DATE_MONTH_TEXT, DATE_MONTH_NUMERIC, ONE_AND_TWO_DIGIT_YEAR_VALUE, FOUR_DIGIT_YEAR_VALUE, DATE_QUALIFIER}
;

FULL_DMY:
	(['on' | DATE_QUALIFIER] ['"'] (DATE_DAY  ['/'] (DATE_MONTH_TEXT|DATE_MONTH_NUMERIC)) ['/'] ([YEAR_KEYWORDS] (ONE_AND_TWO_DIGIT_YEAR_VALUE | FOUR_DIGIT_YEAR_VALUE)) ['"'])
	{attributes:'private, atomic'}
	{action: DATE_DAY, DATE_MONTH_TEXT, DATE_MONTH_NUMERIC, ONE_AND_TWO_DIGIT_YEAR_VALUE, FOUR_DIGIT_YEAR_VALUE, DATE_QUALIFIER}
;

FULL:
   FULL_MDY | (FULL_YMD - FULL_MDY) | ((FULL_DMY - FULL_MDY) - FULL_YMD) | (((FULL_YDM - FULL_MDY) - FULL_YMD) - FULL_DMY)
	{attributes:'private, atomic'}

        <Policies Type="java.lang.String"><![CDATA[PAST_DAY_MONTH:
	('last' DATE_MONTH_TEXT) | ('last' ['week'] WEEKDAY) | (WEEKDAY ['of'] 'last'~'week')
   {attributes: 'private'}
	{action: setTag('past'), WEEKDAY, DATE_MONTH_TEXT}
;

pastDayOffset:
	['the'] 'day' 'before'
   {attributes: 'private'}
	{action: addElement('offset', '2')}
;

pastDay:
   [pastDayOffset] 'yesterday'
   {attributes: 'private'}
	{action: setTag('day'), pastDayOffset}
;

futureDayOffset:
	['the'] 'day' 'after'
   {attributes: 'private'}
	{action: addElement('offset', '2')}
;

futureDay:
   [futureDayOffset] 'tomorrow'
   {attributes: 'private'}
	{action: setTag('day'), futureDayOffset}
;

YESTERDAY:
	pastDay
   {attributes: 'private'}
	{action: setTag('past'), pastDay}
;

PAST_PERIOD_:
	['a'] (PERIOD|OFFSETED_PERIOD) 'ago'
	{attributes: 'private'}
;

PAST_PERIODS:
	[WEEKDAY] PAST_PERIOD_ | PAST_PERIOD_ WEEKDAY |
   (('last'|'past') - (&lt;exists&gt;(('in'|'since') ['the']) ('last'|'past'))) (PERIOD|AbsoluteDate|WEEKDAY|OFFSETED_PERIOD) |
   (&lt;exists&gt; (('since' ['the']['past'|'last']) | ('in' ~ ['the'] ~ ('past'|'last')))  (PERIOD|OFFSETED_PERIOD|AbsoluteDate|WEEKDAY))
   {attributes: 'private'}
	{action: setTag('past'), PAST_PERIOD_, WEEKDAY, PERIOD, AbsoluteDate, OFFSETED_PERIOD}
;

CURRENT_DAYS:
	(['this'~['week' ['on']]] WEEKDAY) | TODAY
   {attributes: 'private'}
	{action: setTag('current'), WEEKDAY, TODAY}
;

CURRENT_PERIODS:
	'this' PERIOD
   {attributes: 'private'}
	{action: setTag('current'), PERIOD}
;

BY_DAY: /*e.g. 'by tuesday''*/
	((&lt;exists&gt; 'by') WEEKDAY)
   {attributes: 'private'}
	{action: setTag('defaultToFuture'), WEEKDAY}
;

FUTURE_DAYS: /*e.g. 'next tuesday' or 'tuesday next week'*/
	(['by'] 'next' ['coming'] ['week'] WEEKDAY) | (WEEKDAY ['coming'|'of'] 'next'~'week')
   {attributes: 'private'}
	{action: setTag('future'), WEEKDAY}
;

FUTURE_MONTH:
	'next' ['coming'] DATE_MONTH_TEXT
   {attributes: 'private'}
	{action: setTag('future'), DATE_MONTH_TEXT}
;

FORTNIGHT:
   'in a fortnight'
   {attributes: 'private'}
   {action: setTag('day'), addElement('offset', '14')}
;

TOMORROW:
	futureDay
   {attributes: 'private'}
	{action: setTag('future'), futureDay}
;

FUTURE_PERIODS:
	OFFSETED_PERIOD 'later' | ORDINAL_PERIOD | FORTNIGHT |
   ('next' - (&lt;exists&gt;(('in' ['the'])|'by') 'next')) (PERIOD|OFFSETED_PERIOD|AbsoluteDate) |
   (((&lt;exists&gt; (('by'|'in') ['the']['next']))) (PERIOD | OFFSETED_PERIOD | AbsoluteDate))
   {attributes: 'private'}
	{action: setTag('future'), PERIOD, OFFSETED_PERIOD, ORDINAL_PERIOD, FORTNIGHT, AbsoluteDate}
;

SINGLE_TENSES:
   (CURRENT_DAYS | CURRENT_PERIODS | PAST_PERIODS | FUTURE_PERIODS | FUTURE_DAYS | BY_DAY | FUTURE_MONTH | PAST_DAY_MONTH | YESTERDAY | TOMORROW)+
;]]></Policies>
